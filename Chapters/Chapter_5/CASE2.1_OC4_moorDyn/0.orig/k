FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include        "$FOAM_CASE/0/include/boundaryConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;

boundaryField
{

        "(inlet|outlet|walls)"
        {
                type    fixedValue;
                value   $internalField;
        }

        atmosphere
        {
                type            inletOutlet;
                inletValue      $internalField;
                value           $internalField;
        }

	floater
        {
                type    kqRWallFunction;
                value   uniform 1e-10; //dummy
        }
}


