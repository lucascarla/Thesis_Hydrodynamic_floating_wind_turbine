FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    cyclic1
    {
        type            cyclic;
    }
    cyclic2
    {
        type            cyclic;
    }
    bottom
    {
        type            slip;
    }
    atmosphere
    {
        type            totalPressure;
        p0              uniform 0;
    }
    
    defaultFaces
    {
        type            empty;
    }
}


